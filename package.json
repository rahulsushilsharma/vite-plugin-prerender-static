{
  "name": "vite-plugin-prerender-static",
  "type": "module",
  "version": "0.1.2",
  "description": "Static prerendering plugin for Vite with SEO, meta tags, and multi-route support.",
  "author": "Rahul Sharma <rahu8299@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/rahulsushilsharma/vite-plugin-prerender-static#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahulsushilsharma/vite-plugin-prerender-static.git"
  },
  "bugs": {
    "url": "https://github.com/rahulsushilsharma/vite-plugin-prerender-static/issues"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp --commit --tag --push",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "spa-prerender-static": "^0.1.0"
  },
  "peerDependencies": {
    "vite": "^4 || ^5 || ^6 || ^7"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "bumpp": "^10.3.2",
    "tsdown": "^0.18.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "prerender",
    "static-site",
    "seo",
    "ssg",
    "spa",
    "meta-tags"
  ]
}
